# .env.example
# Copy this file to .env and fill in your actual values

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1

# Vertex AI Search Datastore
VERTEX_AI_SEARCH_DATASTORE_ID=YOUR_DATASTORE_ID_HERE

# Service Account Key Path (for Vertex AI)
GOOGLE_APPLICATION_CREDENTIALS=./vertex-key.json

# OAuth Credentials Path (for Workspace APIs)
GOOGLE_OAUTH_CREDENTIALS=./credentials.json

# Vertex AI Search (Phase 3)
VERTEX_AI_SEARCH_DATASTORE_ID=your-datastore-id

# BigQuery (Phase 3)
BIGQUERY_DATASET_ID=student_records

# Model Configuration
ORCHESTRATOR_MODEL=gemini-2.0-flash-exp
SPECIALIST_MODEL=gemini-2.0-flash-exp

# Logging
LOG_LEVEL=INFO
ENVIRONMENT=development
